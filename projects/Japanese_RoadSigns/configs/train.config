;./project/Kajima/configs/train.config
[project]
name        = Kajima
owner       = Kajima
dataset     = Kajima

[hardware]
tpu         = None
tpu_zone    = None
gcp_project = None
strategy    = None
use_xla     = None

[model]
name        = efficientdet-d0
model_dir   = ./projects/Kajima/models/
profile     = None
ckpt        = ./efficientdet-d0

[training]
mode                      = train_and_eval
run_epoch_in_child_process= False
batch_size                = 4
epochs                    = 50
save_checkpoints_steps    = 100
file_pattern              = ./projects/Kajima/train/512x512_kajima.tfrecord

examples_per_epoch        = 64
hparams                   = ./projects/Kajima/configs/default.yaml
cores                     = 0
use_spatial_partition     = False

cores_per_replica         = 2
input_partition_dims      = [1, 2, 1, 1]
tf_random_seed            = False
use_fake_data             = False
training_losses_file      = ./projects/Kajima/train_losses.csv

[validation]
file_pattern            = ./projects/Kajima/valid/512x512_kajima.tfrecord
batch_size              = 4
eval_samples            = 64
iterations_per_loop     = 20
val_json_file           = None
eval_after_train        = True
min_eval_interval       = 180
timeout                 = None
evaluation_results_file = ./projects/Kajima/eval_results.csv

[early_stopping]
patience   = 10

[epoch_change_notifier]
enabled   = True
ipaddress = 127.0.0.1
port      = 9999


